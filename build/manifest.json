{
    "manifest_version": 3,
    "name": "Filter & Log Time",
    "version": "1.0",
    "description": "Work Time Logger",
    "icons": {
        "16": "./wl-logo.png",
        "32": "./wl-logo.png",
        "48": "./wl-logo.png",
        "128": "./wl-logo.png"
    },
    "permissions": ["scripting", "activeTab", "storage", "cookies"],
    "host_permissions": ["https://*/*"],
    "action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    }
}
